{
    "name": "qhweb/think-library",
    "description": "think Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "qhweb",
            "email": "qhweb@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0",
        "topthink/think-view": "^1.0",
        "topthink/think-helper": "^3.0.0"
    },
    "autoload":{
        "files": [
          "src/common.php"
        ],
        "psr-4":{
            "YYCms\\":"src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "YYCms\\Library"
            ],
            "config":{
                "addons": "src/addons/config.php"
            }
        }
    }
}
